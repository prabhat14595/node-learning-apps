<!DOCTYPE html>
<html>
    {{>header}}
    {{>navbar}}
    
    <body>

        <div class="container center-align" style="margin-top:1cm">
            <i class="large material-icons  green-text lighten-2">opacity</i>
        </div> 

 <div class="container row" style="margin-top:20px">
    <form class="col s12" method="POST" action="/">
      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons green-text prefix">location_on</i>
          <input id="icon_prefix" type="text" name="city" >
          <label for="icon_prefix">City</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons green-text prefix">location_searching</i>
          <input id="icon_telephone" type="number" name="zipcode" >
          <label for="icon_telephone">Zipcode</label>
        </div>
      </div>
      <div class="input-field align-center" style="align-content:center">
          <input class="waves-effect waves-light btn algin-center" id="submit" type="submit" value="submit">
          <label for="icon_telephone">Submit</label>
        </div> 
    </form>
  </div>
</body>



    {{> footer}}

</html>